onEvent("button39", "click", function( ) {
  playSound("sound://category_app/app_button_slide_cool_2.mp3", false);
  hideElement("TvAd");
  hideElement("TvAdText");
  hideElement("TvNews");
  hideElement("TvNewsText");
  showElement("TvCartoon");
});
onEvent("button38", "click", function( ) {
  playSound("sound://category_app/app_button_slide_cool_2.mp3", false);
  hideElement("TvCartoon");
  hideElement("TvAd");
  hideElement("TvAdText");
  showElement("TvNews");
  showElement("TvNewsText");
  setTimeout(function() {
    hideElement("TvNews");
    hideElement("TvNewsText");
    hideElement("TvAd");
    hideElement("TvAdText");
    showElement("TvWARNING");
    showElement("text_area3");
  }, 4000);
});
