//SlimePlane
var SlimePlane = 0;
timedLoop(30000, function() {
  showElement("SlimePlane1");
  setTimeout(function() {
    showElement("Slimeplane2");
    hideElement("SlimePlane1");
  }, 500);
  setTimeout(function() {
    hideElement("Slimeplane2");
    showElement("SlimePlane3");
  }, 1000);
  setTimeout(function() {
    hideElement("SlimePlane3");
    showElement("SlimePlane4");
  }, 1500);
  setTimeout(function() {
    hideElement("SlimePlane4");
  }, 2000);
});
