//Cheat Code
onEvent("button96", "click", function( ) {
  var Cheat = prompt("Ah, cheat code! Enter the cheat code to get some stuff (capitals matter)");
  if (Cheat == "BASIC1") {
    var x = prompt("Gotcha, here are ALL THE BASIC ACHIEVMENTS unlocked!");
    HowToPlay = HowToPlay + 1;
    HowToPlay2 = HowToPlay2 + 1;
    WhatsNew = WhatsNew + 1;
    NoChanging = NoChanging + 1;
    StoryTime = StoryTime + 1;
    HereYourCredits = HereYourCredits + 1;
    Reviewed = Reviewed + 1;
    Unlock = Unlock + 1;
    MusicTruth = MusicTruth + 1;
    SlimePlane = SlimePlane + 1;
    JustAsk = JustAsk + 1;
    Meow = Meow + 1;
    Blossom = Blossom + 1;
    showElement("button8");
    showElement("button69");
    showElement("button9");
    showElement("button18");
    showElement("button35");
    showElement("button61");
    showElement("button75");
    showElement("button77");
    showElement("button79");
    showElement("button51");
    showElement("button10");
    showElement("button71");
    showElement("button74");
  } else if ((Cheat == "adventure1")) {
    var x = prompt("Ok, all Adventure achievments for you");
    Adventure = Adventure + 1;
    Adventure2 = Adventure2 + 1;
    showElement("button26");
    showElement("button27");
    showElement("button28");
    showElement("button29");
    showElement("button82");
    showElement("button81");
    showElement("button86");
    showElement("button88");
  } else if ((Cheat == "TV")) {
    var x = prompt("Actually confused here but ok...");
    PictureTV = PictureTV + 1;
    TerribleCartoon = TerribleCartoon + 1;
    TwoThousandOneNews = TwoThousandOneNews + 1;
    WARNING = WARNING + 1;
    showElement("button46");
    showElement("button47");
    showElement("button48");
    showElement("button45");
  }
});
//Review
onEvent("button67", "click", function( ) {
  playSound("sound://category_app/app_button_slide_cool_2.mp3", false);
  var x = prompt("This will take some time...");
  createRecord("Player Reviews", {Name:getText("ReviewUsername"), Review:(getText("ReviewAnswer"))}, function(record) {
    var x = prompt("Review submitted!");
    if (Reviewed == 0) {
      var x = prompt("Reviewed! unlocked! Check the achievment section");
      Reviewed = Reviewed + 1;
      showElement("button75");
      playSound("sound://category_app/app_button_slide_cool_2.mp3", false);
    }
  });
  createRecord("Player Stars", {Name:getText("ReviewUsername"), Stars:getNumber("text_area16")}, function(record) {
    var x = prompt("1-10 rating scale info submitted!");
  });
});
