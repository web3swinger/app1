    }, 4000);
  }
});
//Unlock station
onEvent("button16", "click", function( ) {
  if (getText("text_input1") == "HfdaAD") {
    var x = prompt("Newbie Master unlocked! Check the achievment section");
    showElement("button8");
    setText("text_input1", "");
  } else if ((getText("text_input1") == "CHNGLL")) {
    var x = prompt("What's new? unlocked! Check the achievment section");
    showElement("button9");
    setText("text_input1", "");
  } else if ((getNumber("text_input1") == "65")) {
    var x = prompt("Just ask? unlocked! Check the achievment section");
    showElement("button10");
    setText("text_input1", "");
  } else if ((getText("text_input1") == "UysanbaA")) {
    var x = prompt("No changing unlocked! Check the achievment section");
    showElement("button18");
    setText("text_input1", "");
  } else if ((getText("text_input1") == "Utasg")) {
    var x = prompt("Adventure! unlocked! Check the achievment section");
    showElement("button26");
  } else if ((getText("text_input1") == "YYfdaj")) {
    var x = prompt("Cow unlocked! Check the achievment section");
    showElement("button27");
  } else if ((getText("text_input1") == "LKAuhigA")) {
    var x = prompt("Big spider unlocked! Check the achievment section");
    showElement("button28");
  } else if ((getText("text_input1") == "HHHyfsa")) {
    var x = prompt("GOTTEN!! unlocked! Check the achievment section");
    showElement("button29");
  } else if ((getText("text_input1") == "BaQQg")) {
    var x = prompt("Adventure! unlocked! Check the achievment section!");
    showElement("button35");
  } else if ((getText("text_input1") == "CAFGA")) {
    var x = prompt("Here Your Credits! unlocked! Check the achievment section!");
    showElement("button61");
  } else if ((getText("text_input1") == "MAn")) {
    showElement("button35");
    var x = prompt("Story Time! unlocked! Check the achievment section!");
  } else if ((getText("text_input1") == "HfdaAD221")) {
    showElement("button69");
    var x = prompt("Advanced Newbie Master unlocked! Check the achievment section!");
  } else if ((getText("text_input1") == "LKAbsa")) {
    showElement("button71");
    var x = prompt("Meow! unlocked! Check the achievment section!");
  } else if ((getText("text_input1") == "MASDhusa43")) {
    showElement("button74");
    var x = prompt("Blossom! unlocked! Check the achievment section!");
  } else if ((getText("text_input1") == "RevieAAg")) {
    showElement("button75");
    var x = prompt("Reviewed! unlocked! Check the achievment section!");
  } else if ((getText("text_input1") == "Musayta")) {
    showElement("button79");
    var x = prompt("You got the achievment yes, but are you listening to music right now, 'cause the achievment is based of music");
  } else if ((getText("text_input1") == "ImageJdsa")) {
    var x = prompt("Unlocking Stuff unlocked! Check the achievment section!");
    showElement("button77");
  } else if ((getText("text_input1") == "BeasAd")) {
    var x = prompt("A Second Adventure! unlocked! Check the achievment section!");
    showElement("button82");
  } else if ((getText("text_input1") == "EaGota")) {
    var x = prompt("Image Guesser!! Yeah!! (you unlocked the achievment, don't worry)");
    showElement("button81");
    showElement("button81");
  } else if ((getText("text_input1") == "IASj")) {
    var x = prompt("Gotcha! unlocked! Check the achievment section!");
    showElement("button86");
  } else if ((getText("text_input1") == "ItaJeTT")) {
    var x = prompt("Trick... unlocked! Check the achievment section!");
    showElement("button88");
  } else {
    setText("text_input1", "INVALID CODE. NO SUCH CODE EXISTS INGAME.");
  }
});
